<div class="page">
	<div class="container">
		<div class="row">
			<div class="col-6">
				<h1>Your result</h1>
			</div>
			<div class="col-6">
				<div class="category">
					<div class="summary">Summary</div>
					{{#each summary}}
					<div>{{category}} <span class="category_score">{{score}}</span> / 100</div>
					{{/each}}
				</div>
				<button>Continue</button>
			</div>
		</div>
	</div>
</div>